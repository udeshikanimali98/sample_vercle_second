    "mongoose": "^8.5.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemon": "^3.0.1",
    "passport": "^0.7.0",
